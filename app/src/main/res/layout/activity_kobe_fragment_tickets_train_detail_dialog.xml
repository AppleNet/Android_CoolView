<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@color/white">
    
	<RelativeLayout 
	    android:id="@+id/high_speed_rail"
	    android:layout_width="fill_parent"
	    android:layout_height="40dp"
	    android:background="@drawable/list_item_bg"
	    android:layout_marginLeft="20dp"
	    android:layout_marginRight="20dp"
	    android:layout_marginTop="10dp">
	    <TextView
	        android:id="@+id/high_speed_rail_tv" 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:text="@string/high_speed_rail"
	        android:textColor="@color/black"/>
	    <CheckBox
	        android:id="@+id/high_speed_rail_ck" 
	        style="@style/CustomCheckboxTheme"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:layout_centerVertical="true"
	        android:layout_gravity="end"/>
	</RelativeLayout>
	<View
        android:layout_width="fill_parent"
        android:layout_height="@dimen/view_divider"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@color/black_divider" />
	<RelativeLayout 
	    android:id="@+id/motor_car"
	    android:layout_width="fill_parent"
	    android:layout_height="40dp"
	    android:background="@drawable/list_item_bg"
	    android:layout_marginLeft="20dp"
	    android:layout_marginRight="20dp">
	    <TextView 
	        android:id="@+id/motor_car_tv"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:text="@string/motor_car"
	        android:textColor="@color/black"/>
	    <CheckBox 
	        android:id="@+id/motor_car_ck" 
	        style="@style/CustomCheckboxTheme"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:layout_centerVertical="true"
	        android:layout_gravity="end"/>
	</RelativeLayout>
	<View
        android:layout_width="fill_parent"
        android:layout_height="@dimen/view_divider"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@color/black_divider" />
	<RelativeLayout 
	    android:id="@+id/non_stop"
	    android:layout_width="fill_parent"
	    android:layout_height="40dp"
	    android:background="@drawable/list_item_bg"
	    android:layout_marginLeft="20dp"
	    android:layout_marginRight="20dp">
	    <TextView 
	        android:id="@+id/non_stop_tv"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:text="@string/non_stop"
	        android:textColor="@color/black"/>
	    <CheckBox
	        android:id="@+id/non_stop_ck"  
	        style="@style/CustomCheckboxTheme"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:layout_centerVertical="true"
	        android:layout_gravity="end"/>
	</RelativeLayout>
	<View
        android:layout_width="fill_parent"
        android:layout_height="@dimen/view_divider"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@color/black_divider" />
	<RelativeLayout 
	    android:id="@+id/express_train"
	    android:layout_width="fill_parent"
	    android:layout_height="40dp"
	    android:background="@drawable/list_item_bg"
	    android:layout_marginLeft="20dp"
	    android:layout_marginRight="20dp">
	    <TextView 
	        android:id="@+id/express_train_tv"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:text="@string/express_train"
	        android:textColor="@color/black"/>
	    <CheckBox
	        android:id="@+id/express_train_ck" 
	        style="@style/CustomCheckboxTheme"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:layout_centerVertical="true"
	        android:layout_gravity="end"/>
	</RelativeLayout>
	<View
        android:layout_width="fill_parent"
        android:layout_height="@dimen/view_divider"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@color/black_divider" />
	<RelativeLayout 
	    android:id="@+id/quick_train"
	    android:layout_width="fill_parent"
	    android:layout_height="40dp"
	    android:background="@drawable/list_item_bg"
	    android:layout_marginLeft="20dp"
	    android:layout_marginRight="20dp">
	    <TextView 
	        android:id="@+id/quick_train_tv"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:text="@string/quick_train"
	        android:textColor="@color/black"/>
	    <CheckBox
	        android:id="@+id/quick_train_ck" 
	        style="@style/CustomCheckboxTheme"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:layout_centerVertical="true"
	        android:layout_gravity="end"/>
	</RelativeLayout>
	<View
        android:layout_width="fill_parent"
        android:layout_height="@dimen/view_divider"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@color/black_divider" />
	<RelativeLayout 
	    android:id="@+id/other"
	    android:layout_width="fill_parent"
	    android:layout_height="40dp"
	    android:background="@drawable/list_item_bg"
	    android:layout_marginLeft="20dp"
	    android:layout_marginRight="20dp">
	    <TextView
	        android:id="@+id/other_tv" 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:text="@string/other"
	        android:textColor="@color/black"/>
	    <CheckBox 
	        android:id="@+id/other_ck"
	        style="@style/CustomCheckboxTheme"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:layout_centerVertical="true"
	        android:layout_gravity="end"/>
	</RelativeLayout>
	<View
        android:layout_width="fill_parent"
        android:layout_height="@dimen/view_divider"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@color/black_divider" />
	<TextView 
	    android:id="@+id/ok_tv"
	    android:layout_width="fill_parent"
	    android:layout_height="50dp"
	    android:layout_marginLeft="20dp"
	    android:layout_marginRight="20dp"
	    android:gravity="center"
	    android:background="@drawable/kobe_tickets_dialog_ok"
	    android:clickable="true"
	    android:text="@string/tpl_ok"
	    android:textColor="@color/black"/>
</LinearLayout>
